<h1>sign up</h1>
<%= form_with model: @user,url: user_path ,local: true do |f| %>
<div class="field">
    <%= f.label :uid %>
    <%= f.text_field :uid %>
</div>
<div class="field">
    <%= f.label :password %>
    <%= f.text_field :password %>
</div>
<div>
    <%= f.submit "sign up" %>
</div>
<%= link_to "login",login_path %>
<% end %>